{
  packageRules: [
    {
      description: 'Auto merge containers at night',
      matchDatasources: [
        'docker',
      ],
      automerge: true,
      automergeType: 'branch',
      requiredStatusChecks: null,
      matchUpdateTypes: [
        'minor',
        'patch',
        'digest',
      ],
      automergeSchedule: [
        'after 10pm and before 5am',
      ],
    },
    {
      description: 'Auto merge containers anytime',
      matchDatasources: [
        'docker',
      ],
      automerge: true,
      automergeType: 'branch',
      requiredStatusChecks: null,
      matchUpdateTypes: [
        'minor',
        'patch',
        'digest',
      ],
      matchPackageNames: [
        'ghcr.io/onedr0p/prowlarr-nightly',
      ],
    },
    {
      description: 'Auto merge GitHub Actions',
      matchDatasources: [
        'github-tags',
      ],
      automerge: true,
      automergeType: 'branch',
      requiredStatusChecks: null,
      matchUpdateTypes: [
        'minor',
        'patch',
      ],
      matchPackageNames: [
        'lycheeverse/lychee-action',
        'renovatebot/github-action',
      ],
    },
  ],
}
