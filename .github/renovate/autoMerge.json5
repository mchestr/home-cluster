{
  "packageRules": [
    {
      "matchDatasources": [
        "docker",
        "github-actions",
        "github-releases",
        "github-tags",
        "helm"
      ],
      "automerge": true,
      "automergeType": "branch",
      "requiredStatusChecks": null,
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "matchPackageNames": [
        "ghcr.io/k8s-at-home/lidarr",
        "ghcr.io/k8s-at-home/prowlarr-nightly",
        "ghcr.io/k8s-at-home/readarr",
        "renovatebot/github-action"
      ]
    }
  ]
}
