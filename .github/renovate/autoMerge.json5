{
  packageRules: [
    {
      description: "Auto merge container digests",
      matchDatasources: ["docker"],
      automerge: true,
      automergeType: "branch",
      requiredStatusChecks: null,
      matchUpdateTypes: ["digest"],
      matchPackagePatterns: [
        "ghcr.io/home-operations",
        "ghcr.io/bjw-s",
        "ghcr.io/mchestr",
      ],
    },
    {
      description: ["Auto-merge GitHub Actions"],
      matchManagers: ["github-actions"],
      automerge: true,
      automergeType: "branch",
      matchUpdateTypes: ["minor", "patch", "digest"],
      minimumReleaseAge: "3 days",
      ignoreTests: true,
    },
    {
      matchDatasources: [
        "docker",
        "github-actions",
        "github-releases",
        "github-tags",
        "helm",
      ],
      automerge: true,
      automergeType: "branch",
      requiredStatusChecks: null,
      matchUpdateTypes: ["minor", "patch", "digest"],
      matchDepNames: [
        "ghcr.io/esphome/esphome",
        "ghcr.io/fallenbagel/jellyseerr",
        "ghcr.io/home-operations/bazarr",
        "ghcr.io/home-operations/plex",
        "ghcr.io/home-operations/prowlarr",
        "ghcr.io/home-operations/qbittorrent",
        "ghcr.io/home-operations/radarr",
        "ghcr.io/home-operations/sabnzbd",
        "ghcr.io/home-operations/sonarr",
        "ghcr.io/home-operations/tautulli",
        "ghcr.io/koenkk/zigbee2mqtt",
        "ghcr.io/paperless-ngx/paperless-ngx",
        "ghcr.io/twin/gatus",
        "ghcr.io/wizarrrr/wizarr",
        "ghcr.io/zwave-js/zwave-js-ui",
        "grafana",
        "wordpress",
      ],
      schedule: ["after 2am and before 6am"],
      stabilityDays: 1,
    },
  ],
}
