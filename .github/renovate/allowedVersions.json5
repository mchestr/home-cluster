{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["postgresql", "postgres-backup-local"],
      "allowedVersions": "<18"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["ghcr.io/blakeblackshear/frigate"],
      "versioning": "semver",
      "ignoreUnstable": false
    }
  ]
}
